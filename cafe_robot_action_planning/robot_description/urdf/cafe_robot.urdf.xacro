<?xml version="1.0"?>
<robot name="cafe_robot" xmlns:xacro="http://www.ros.org/wiki/xacro">

    <xacro:include filename="inertial_macros.xacro"/>
    <xacro:include filename="gazebo_plugins.xacro"/>
    <xacro:include filename="inertial_macros.xacro"/>
    
    <!--                Dimensions              -->
    <xacro:property name="chassis_length" value="0.45"/>
    <xacro:property name="chassis_width" value="0.4"/>
    <xacro:property name="chassis_height" value="0.2"/>
    <xacro:property name="wheel_thk" value="0.05"/>
    <xacro:property name="wheel_radius" value="0.1"/>
    <xacro:property name="lidar_radius" value="0.06"/>
    <xacro:property name="lidar_height" value="0.05"/>
    <xacro:property name="support_height" value="0.65"/>
    <xacro:property name="support_radius" value="0.01"/>
    <xacro:property name="support_pos_z" value="0.425"/>
    

    <!--                Base Link               -->
    <link name="base_link"/>

    <joint name="base_footprint_joint" type="fixed">
        <parent link="base_link"/>
        <child link="base_footprint"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
    </joint>

    <link name="base_footprint"/>



    <!--                Chassis Joint               -->
    <joint name="chassis_joint" type="fixed">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <parent link="base_link"/>
        <child link="chassis_link"/>
        <axis xyz="0.0 0.0 0.0"/>
    </joint>

    <link name="chassis_link">
        <xacro:box_inertia mass="2.5" x="${chassis_length}" y="${chassis_width}" z="${chassis_height}">
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </xacro:box_inertia>
        <visual>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <box size="${chassis_length} ${chassis_width} ${chassis_height}"/>
            </geometry>
            <material name="dark">
                <color rgba="1.0 1.0 1.0 1.0"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <box size="${chassis_length} ${chassis_width} ${chassis_height}"/>
            </geometry>
        </collision>
    </link>
    


    <!--            Right Wheel Joint                -->
    <joint name="wheel_R_joint" type="continuous">
        <parent link="base_link"/>
        <child link="wheel_R_link"/>
        <axis xyz="0 0 1"/>
        <origin xyz="0 0.225 -0.03" rpy="1.57 0 0"/>
    </joint>
    
    <link name="wheel_L_link">
        <xacro:cylinder_inertia mass="1.0" length="${wheel_thk}" radius="${wheel_radius}">
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </xacro:cylinder_inertia>
        <visual>
            <origin xyz="0.0 0.0 0.0" rpy=" 0.0 0.0 0.0"/>
            <geometry>
                <cylinder radius="${wheel_radius}" length="${wheel_thk}"/> 
            </geometry>
            <material name="grey">
                <color rgba="0.1 0.7 0.7 1.0"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <cylinder radius="${wheel_radius}" length="${wheel_thk}"/> 
            </geometry>
        </collision>
    </link>



    <!--                Left Wheel Joint               -->
    <joint name="wheel_L_joint" type="continuous">
        <parent link="base_link"/>
        <child link="wheel_L_link"/>
        <axis xyz="0 0 1"/>
        <origin xyz="0 -0.225 -0.03" rpy="1.57 0 0"/>
    </joint>
    
    <link name="wheel_R_link">
        <xacro:cylinder_inertia mass="1.0" length="${wheel_thk}" radius="${wheel_radius}">
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </xacro:cylinder_inertia>
        <visual>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <cylinder radius="${wheel_radius}" length="${wheel_thk}"/> 
            </geometry>
            <material name="grey">
                <color rgba="0.1 0.7 0.7 1.0"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <cylinder radius="${wheel_radius}" length="${wheel_thk}"/> 
            </geometry>
        </collision>
    </link>



    <!--                Caster wheel joints & links              -->
    <joint name="caster_wheelF_joint" type="fixed">
        <parent link="chassis_link"/>
        <child link="caster_wheelF"/>
        <origin xyz="0.15 0 -0.1" rpy="0.0 0 0"/>
    </joint>
    
    <link name="caster_wheelF">
        <xacro:sphere_inertia mass="0.15" radius="0.03">
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </xacro:sphere_inertia>
        <visual>
            <origin xyz="0.0 0.0 0.0" rpy=" 0.0 0.0 0.0"/>
            <geometry>
                <sphere radius="0.03"/>
            </geometry>
            <material name="black">
                <color rgba="0.0 0.0 0.0 1.0"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <sphere radius="0.03"/>
            </geometry>
        </collision>
    </link>

    <gazebo reference="caster_wheelF">
        <material>Gazebo/White</material>
        <mu1 value="0.001"/>
        <mu2 value="0.001"/>
    </gazebo>

    <joint name="caster_wheelB_joint" type="fixed">
        <parent link="chassis_link"/>
        <child link="caster_wheelB"/>
        <origin xyz="-0.15 0 -0.1" rpy="0.0 0 0"/>
    </joint>
    
    <link name="caster_wheelB">
        <xacro:sphere_inertia mass="0.15" radius="0.03">
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </xacro:sphere_inertia>
        <visual>
            <origin xyz="0.0 0.0 0.0" rpy=" 0.0 0.0 0.0"/>
            <geometry>
                <sphere radius="0.03"/>
            </geometry>
            <material name="black">
                <color rgba="0.0 0.0 0.0 1.0"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <sphere radius="0.03"/>
            </geometry>
        </collision>
    </link>

    <gazebo reference="caster_wheelB">
        <material>Gazebo/White</material>
        <mu1 value="0.001"/>
        <mu2 value="0.001"/>
    </gazebo>



    <!--                Laser joint & link              -->
    <joint name="lidar_joint" type="fixed">
        <parent link="chassis_link"/>
        <child link="laser_frame"/>
        <origin xyz="0 0 0.125" rpy="0.0 0 0"/>
    </joint>
    
    <link name="laser_frame">
        <xacro:cylinder_inertia mass="0.200" length="${lidar_height}" radius="${lidar_radius}">
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </xacro:cylinder_inertia>
        <visual>
            <origin xyz="0.0 0.0 0.0" rpy=" 0.0 0.0 0.0"/>
            <geometry>
                <cylinder radius="${lidar_radius}" length="${lidar_height}"/> 
            </geometry>
            <material name="orange">
                <color rgba="1.0 0.3 0.0 1.0"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <cylinder radius="${lidar_radius}" length="${lidar_height}"/> 
            </geometry>
        </collision>
    </link>



    <!--            Support Frames              -->
    <joint name="support1_joint" type="fixed">
        <parent link="chassis_link"/>
        <child link="support1"/>
        <origin xyz="0.18 0.15 ${support_pos_z}" rpy="0.0 0.0 0.0"/>
    </joint>

    <link name="support1">
        <xacro:cylinder_inertia mass="0.3" length="${support_height}" radius="${support_radius}">
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </xacro:cylinder_inertia>
        <visual>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <cylinder radius="${support_radius}" length="${support_height}"/>
            </geometry>
            <material name="black">
                <color rgba="0.0 0.0 0.0 1.0"/>
                <texture />
            </material>
        </visual>
        <collision>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <cylinder radius="0.02" length="0.2"/>
            </geometry>
        </collision>
    </link>

    <joint name="support2_joint" type="fixed">
        <parent link="chassis_link"/>
        <child link="support2"/>
        <origin xyz="-0.18 -0.15 ${support_pos_z}" rpy="0.0 0.0 0.0"/>
    </joint>

    <link name="support2">
        <xacro:cylinder_inertia mass="0.3" length="${support_height}" radius="${support_radius}">
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </xacro:cylinder_inertia>
        <visual>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <cylinder radius="${support_radius}" length="${support_height}"/>
            </geometry>
            <material name="black">
                <color rgba="0.0 0.0 0.0 1.0"/>
                <texture />
            </material>
        </visual>
        <collision>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <cylinder radius="0.02" length="0.2"/>
            </geometry>
        </collision>
    </link>
    
    <joint name="support3_joint" type="fixed">
        <parent link="chassis_link"/>
        <child link="support3"/>
        <origin xyz="-0.18 0.15 ${support_pos_z}" rpy="0.0 0.0 0.0"/>
    </joint>

    <link name="support3">
        <xacro:cylinder_inertia mass="0.3" length="${support_height}" radius="${support_radius}">
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </xacro:cylinder_inertia>
        <visual>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <cylinder radius="${support_radius}" length="${support_height}"/>
            </geometry>
            <material name="black">
                <color rgba="0.0 0.0 0.0 1.0"/>
                <texture />
            </material>
        </visual>
        <collision>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <cylinder radius="0.02" length="0.2"/>
            </geometry>
        </collision>
    </link>

    <joint name="support4_joint" type="fixed">
        <parent link="chassis_link"/>
        <child link="support4"/>
        <origin xyz="0.18 -0.15 ${support_pos_z}" rpy="0.0 0.0 0.0"/>
    </joint>

    <link name="support4">
        <xacro:cylinder_inertia mass="0.3" length="${support_height}" radius="${support_radius}">
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </xacro:cylinder_inertia>
        <visual>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <cylinder radius="${support_radius}" length="${support_height}"/>
            </geometry>
            <material name="black">
                <color rgba="0.0 0.0 0.0 1.0"/>
                <texture />
            </material>
        </visual>
        <collision>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <cylinder radius="0.02" length="0.2"/>
            </geometry>
        </collision>
    </link>


    <!--                Level Frames              -->
    <joint name="level1_joint" type="fixed">
        <parent link="chassis_link"/>
        <child link="level1"/>
        <origin xyz="0 0 0.25" rpy="0.0 0.0 0.0"/>
    </joint>

    <link name="level1">
        <xacro:cylinder_inertia mass="0.3" length="${wheel_thk}" radius="${wheel_radius}">
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </xacro:cylinder_inertia>
        <visual>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <box size = "${chassis_length} ${chassis_width} 0.02"/>
            </geometry>
            <material name="dark">
                <color rgba="0.3 0.3 0.3 1.0"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <box size = "0 0 0"/>
            </geometry>
        </collision>
    </link>

    <joint name="level2_joint" type="fixed">
        <parent link="chassis_link"/>
        <child link="level2"/>
        <origin xyz="0 0 0.5" rpy="0.0 0.0 0.0"/>
    </joint>

    <link name="level2">
        <xacro:cylinder_inertia mass="0.3" length="${wheel_thk}" radius="${wheel_radius}">
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </xacro:cylinder_inertia>
        <visual>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <box size = "${chassis_length} ${chassis_width} 0.02"/>
            </geometry>
            <material name="dark">
                <color rgba="0.3 0.3 0.3 1.0"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <box size = "0 0 0"/>
            </geometry>
        </collision>
    </link>


    <joint name="level3_joint" type="fixed">
        <parent link="chassis_link"/>
        <child link="level3"/>
        <origin xyz="0 0 0.75" rpy="0.0 0.0 0.0"/>
    </joint>

    <link name="level3">
        <xacro:cylinder_inertia mass="0.3" length="${wheel_thk}" radius="${wheel_radius}">
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </xacro:cylinder_inertia>
        <visual>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <box size = "${chassis_length} ${chassis_width} 0.02"/>
            </geometry>
           <material name="dark">
                <color rgba="0.3 0.3 0.3 1.0"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <box size = "0 0 0"/>
            </geometry>
        </collision>
    </link>

</robot>